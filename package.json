{
  "name": "bot-detection",
  "version": "0.1.1",
  "description": "Client and server bot detection utilities with Express middleware and optional ML scoring.",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./auto": {
      "types": "./dist/client/auto.d.ts",
      "require": "./dist/client/auto.js",
      "default": "./dist/client/auto.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "npm run build && node --test tests/**/*.test.js",
    "demo": "node examples/express-demo/server.js"
  },
  "keywords": [
    "bot",
    "detection",
    "express",
    "middleware",
    "fingerprint",
    "heuristics"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "express": ">=4.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  }
}
